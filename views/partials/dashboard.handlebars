<body>

<h1 class= 'section-heading' style='text-align: center;'>Manager Dashboard</h1>
<p><h3 style='text-align: center;'>Employees</h3></p>
    <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>ID</th>
                <th>First name</th>
                <th>Last Name</th>
                <th>E-mail</th>
                <th>Phone #</th>
                <th>Address</th>
                <th>Username</th>
                <th>Password</th>
                <th>Admin (y/n)</th>
                <th>Created At</th>
                <th>Updated At</th>
            </tr>
        </thead>
    
     {{#each Employees.DataValues }}

            <tr>
                <td> {{ dataValues.first_name }} </td>
               {{!--  <td> {{ this.first_name }} </td>
                <td> {{ this.last_name }} </td> --}}
                <td>Systems Administrator</td>
                <td>555-555-5555</td>
                <td>123 Honey Ln.</td>
                <td>Username</td>
                <td>Password</td>
                <td>Yes</td>
                <td>Today</td>
                <td>Also Today</td>
            </tr>

      {{/each }}      

        </tbody>
    </table>
